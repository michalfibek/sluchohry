parameters:
	songDir: www/assets/sounds/songs/
	uploadDir: uploads/

php:
	date.timezone: Europe/Prague


application:
	errorPresenter: Base:Error
	mapping:
		*: App\Module\*\Presenters\*Presenter

session:
	expiration: 14 days


services:
	- App\Model\UserManager
	- App\Model\SongStorage
	router: App\RouterFactory::createRouter
